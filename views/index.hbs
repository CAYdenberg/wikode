<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Wikode</title>
    {{#each stylesheets}}
      <link rel="stylesheet" href="{{ this }}" type="text/css">
    {{/each}}
  </head>

  <body>
    <div id="mount-point" data-template="{{ template }}">
      {{{ reactHtml }}}
    </div>

    <script type="text/javascript">
      window.globals = {{{json data}}}
    </script>
    {{#each scripts}}
      <script src="{{ this }}" type="text/javascript"></script>
    {{/each}}

  </body>
</html>
