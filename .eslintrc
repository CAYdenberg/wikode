{
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "sourceType": "module",
  "env": {
    "es6": true,
    "commonjs": true,
    "browser": true
  },
  "globals": {
    "globals": true,
    "console": true,
    "process": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "no-unused-vars": [2, { "varsIgnorePattern" : "React" }]
  }
}
